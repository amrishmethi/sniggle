    <script> 
        if (document.body.contains(document.getElementById('Body_txtMetatitle'))) {
            $('#Body_txtMetatitle').keyup(function () {
                var max = 70;
                var len = $(this).val().length;
                var char = max - len;
                $('#mtitle').text(char);
                //if (len >= max) {
                //    $('#mtitle').text(' you have reached the limit');
