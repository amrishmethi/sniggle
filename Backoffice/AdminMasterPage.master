            });
            $('#Body_txtMetadescription').keyup(function () {
                var max = 160;
                var len = $(this).val().length;
                var char = max - len;
                $('#mDes').text(char);
            });
            $(document).ready(function () {
                $('#Body_Selectall').prop('checked', false);
                $('#Body_Unselectall').prop('checked', false);
                var max = 70;
                var len = $('#Body_txtMetatitle').val().length;
                var char = max - len;
                $('#mtitle').text(char);

                var max1 = 160;
                var len1 = $('#Body_txtMetadescription').val().length;
                var char1 = max1 - len1;
                $('#mDes').text(char1);
            });
            function DisableButton(id) {

                document.getElementbyId(id).disabled = true;
            }
        } else {
