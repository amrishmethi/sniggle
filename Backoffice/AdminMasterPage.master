    <script type="text/javascript">
        $(document).ready(function () {
            $('#Body_Selectall').checked = false;
            $('#Body_Unselectall').checked = false;
            $('#Body_Selectall').click(function (event) {
                if (this.checked) {
                    $('.checkitem').each(function () {
                        this.checked = true;
                        $('#Body_Unselectall').checked = false;
                    });
                }
            });

            $('#Body_Unselectall').click(function (event) {
                if (this.checked) {
                    $('.checkitem').each(function () {
                        this.checked = false;
                        $('#Body_Selectall').checked = false;
                    });
                }
            });
        });
    </script>
